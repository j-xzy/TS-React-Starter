{
  "name": "ts-react-starter",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./build/devServer.js",
    "build": "webpack-cli --config ./build/webpack/webpack.prod.js",
    "analyze": "webpack-cli --config ./build/webpack/webpack.analyze.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "test": "npm run type-check && jest test --config=jest.config.js",
    "coverage": "jest test --config=jest.config.js --coverage",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "mock": "node ./build/script/doMock.js"
  },
  "sideEffects": [
    "*.css",
    "*.styl",
    "*.scss",
    "*/store/**/*.ts",
    "*/config/**/*.ts",
    "*/lib/ajax.ts"
  ],
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "type-redux": "^2.0.2",
    "type-redux-hook": "^1.0.3",
    "redux-logger": "^3.0.6",
    "immutability-helper": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "open": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^2.2.1",
    "ts-faker": "0.0.5",
    "tslint": "^5.20.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-merge": "^4.2.2"
  }
}
