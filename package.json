{
  "name": "ts-react-demo",
  "version": "0.0.0",
  "scripts": {
    "start": "node ./build/devServer.js",
    "build": "webpack-cli --config ./build/webpack/webpack.prod.js",
    "analyze": "webpack-cli --config ./build/webpack/webpack.analyze.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "npm run type-check && jest test --config=jest.config.js",
    "coverage": "jest test --config=jest.config.js --coverage",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "sideEffects": [
    "*.css",
    "*.styl",
    "*.scss",
    "*/store/**/*.ts",
    "*/config/**/*.ts",
    "*/lib/ajax.ts"
  ],
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "type-redux": "^2.0.2",
    "type-redux-hook": "^1.0.1",
    "redux-logger": "^3.0.6",
    "immutability-helper": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "express": "^4.17.1",
    "file-loader": "^4.1.0",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "opn": "^6.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "^1.3.0",
    "ts-faker": "0.0.4",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-merge": "^4.2.1"
  }
}
